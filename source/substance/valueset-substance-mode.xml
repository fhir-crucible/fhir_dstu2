<ValueSet xmlns="http://hl7.org/fhir" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hl7.org/fhir ../../schema/valueset.xsd">
  <name value="Substance Quantity Mode"/>
  <publisher value="FHIR Project team"/>
  <contact>
    <telecom>  
    <system value="url"/>
    <value value="http://hl7.org/fhir"/>
  </telecom>
  </contact>
  <description value="This value sets indicates whether a given quantity is absolute or relative"/>
  <status value="draft"/>
  <define>
    <system value="http://hl7.org/fhir/substance-quantity-mode"/>
    <caseSensitive value="true"/>    
    <concept>
      <code value="abs"/>
      <display value="Absolute"/>
      <definition value="Quantity values are absolute."/>
    </concept>
    <concept>
      <code value="rel"/>
      <display value="Relative"/>
      <definition value="Quantity values are relative to each other."/>
    </concept>
  </define>
</ValueSet>
